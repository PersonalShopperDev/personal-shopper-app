{
  "expo": {
    "name": "Personal Shopper",
    "slug": "personal-shopper-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/icons/app.icon.png",
    "splash": {
      "image": "./src/assets/images/splash.image.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "buildNumber": "3",
      "bundleIdentifier": "com.outsung.personal-shopper-app",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/app.icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.outsung.personal_shopper_app"
    },
    "web": {
      "favicon": "./src/assets/icons/app.icon.png"
    }
  }
}
